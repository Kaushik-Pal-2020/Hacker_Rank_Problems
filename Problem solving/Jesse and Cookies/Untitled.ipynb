    "import numpy as np\n",
    "def runningMedian(a):\n",
    "    my_list = []\n",
    "    for i in range(len(a)):\n",
    "        arr = np.array(a[0:i+1])\n",
    "        my_list.append(np.median(arr))\n",
    "    return my_list\n",
    "\n",
    "print(runningMedian([12,4,5,3,8,7]))"
   ]
